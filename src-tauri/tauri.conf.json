{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "subs2anki",
  "version": "1.3.1",
  "identifier": "com.zhangqiao.sus2anki",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run sync-version && npm run dev",
    "beforeBuildCommand": "npm run sync-version && npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "subs2anki",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
